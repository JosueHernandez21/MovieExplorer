{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "distDir": "./dist/movie-explorer/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
