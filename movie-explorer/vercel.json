{
  "version": 2,
  "builds": [
    {
      "src": "dist/movie-explorer/browser/**/*",
      "use": "@vercel/static",
      "config": {
        "distDir": "dist/movie-explorer/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"]
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    }
  ]
}
